@model Razor.Models.Cards 
        <div class="col-xl-3 col-lg-4 col-md-6 col-sm-12">     
         <div class="card">         
            <div class="headshot"> <img class="card-img-top" src="~/Assets/images/@Model.Photo" alt=""> 
                
        </div>     
            
        <div class="card-block">  
            
        <h3 class="card-title">@Model.Name</h3>      
        <p class="card-text">@Model.Brief</p>       
        <a href = "@Model.WikiUrl" class="btn btn-primary">Wiki</a>     
                
        </div>   
        </div> 
        </div>
